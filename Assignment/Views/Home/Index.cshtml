@model AccountModel
<div class="d-flex flex-row gap-2 justify-content-center" style="background-color:#D6B5EF ; padding:5px;border-radius:5px;">
    <div>
        <h1>Shivendra</h1>
    </div>
    <div class="d-flex flex-column">
        <partial name="_Details" />
        <button class="btn btn-success">Change Style</button>
    </div>
</div>
<div class="d-flex flex-column justify-content-center align-items-center mt-4 ">
     
        <h3>Print Details</h3>
        <div class="form">
        <form id="countForm" asp-controller="Account" asp-action="GetAccount" method="post" >
            <div>
            <label for="RowCount" style="color:white">Number of Rows:</label>
            <div >
            <input type="number" class="form-control" id="rowCount" name="rowCount"/>
            <span id="error-message" class="text-danger"></span>
            </div>
            <button class="btn btn-success submit mt-3" type="submit">Submit</button>
            </div>
        </form>
    </div>
</div>
