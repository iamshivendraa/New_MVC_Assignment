@model IEnumerable<AccountModel>

    <h1 style="text-align:center">Accounts</h1>

    <table class="table table-striped table-striped ">
        <thead style ="background-color:black; color:white">
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Age</th>
            <th>Salary</th>
            <th>Department</th>
        </thead>     
        @foreach (var obj in Model) {
        <tbody>
            <td>@obj.FirstName</td>
            <td>@obj.LastName</td>
            <td>@obj.Email</td>
            <td>@obj.Age</td>
            <td>@obj.Salary</td>
            <td>@obj.Department</td>
        </tbody>
        }
        
    </table>
